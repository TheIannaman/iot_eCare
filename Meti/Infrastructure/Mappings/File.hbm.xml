<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Meti" namespace="Meti.Domain.Models">

  <class name="File" table="files">
    <id name="Id" column="Id" type="guid">
      <generator class="guid.comb" />
    </id>

    <!--Entity Mapping-->
    <property name="FilepathPhysical" length="500"/>
    <property name="FilepathVirtual" length="500"/>
    <property name="Name" length="500"/>
    <property name="Type" length="500"/>
    <property name="Size" length="500"/>

    <!--Entity Base -->
    <property name="InsertDate" />
    <property name="UpdateDate" />
    <property name="InsertUser" />
    <property name="UpdateUser" />
  </class>
</hibernate-mapping>